SkriptsSection:NewToggle("Auto Jump", "auto jumps", function(state)
if state then
    while true do wait()
game.Players.LocalPlayer.Character.Humanoid.Jump = true --true~jumping
end
else
    while true do wait()
game.Players.LocalPlayer.Character.Humanoid.Jump = false --false~jumpingg
end
end
end)

